// Interpolation function for vertex positions
fn interpolateVertex(iso_level: f32, p1: vec3<f32>, p2: vec3<f32>, val1: f32, val2: f32) -> vec3<f32> {
    if (abs(iso_level - val1) < 0.00001) { return p1; }
    if (abs(iso_level - val2) < 0.00001) { return p2; }
    if (abs(val1 - val2) < 0.00001) { return p1; }

    let mu = (iso_level - val1) / (val2 - val1);
    return vec3<f32>(
        p1.x + mu * (p2.x - p1.x),
        p1.y + mu * (p2.y - p1.y),
        p1.z + mu * (p2.z - p1.z)
    );
}

@compute @workgroup_size(8, 8, 8)
fn main(@builtin(global_invocation_id) global_id: vec3<u32>) {
    let x_size = 100u;
    let y_size = 100u;
    let z_size = 100u;
    let iso_level = 0.5f;

    if (global_id.x >= x_size - 1 || global_id.y >= y_size - 1 || global_id.z >= z_size - 1) {
        return;
    }

    let x = global_id.x;
    let y = global_id.y;
    let z = global_id.z;

    let p: array<vec3<f32>, 8> = array<vec3<f32>, 8>(
        vec3<f32>(f32(x), f32(y), f32(z)),
        vec3<f32>(f32(x + 1u), f32(y), f32(z)),
        vec3<f32>(f32(x + 1u), f32(y + 1u), f32(z)),
        vec3<f32>(f32(x), f32(y + 1u), f32(z)),
        vec3<f32>(f32(x), f32(y), f32(z + 1u)),
        vec3<f32>(f32(x + 1u), f32(y), f32(z + 1u)),
        vec3<f32>(f32(x + 1u), f32(y + 1u), f32(z + 1u)),
        vec3<f32>(f32(x), f32(y + 1u), f32(z + 1u)),
    );

    let v: array<f32, 8> = array<f32, 8>(
        f32(grid.voxels[z * x_size * y_size + y * x_size + x]),
        f32(grid.voxels[z * x_size * y_size + y * x_size + (x + 1u)]),
        f32(grid.voxels[z * x_size * y_size + (y + 1u) * x_size + (x + 1u)]),
        f32(grid.voxels[z * x_size * y_size + (y + 1u) * x_size + x]),
        f32(grid.voxels[(z + 1u) * x_size * y_size + y * x_size + x]),
        f32(grid.voxels[(z + 1u) * x_size * y_size + y * x_size + (x + 1u)]),
        f32(grid.voxels[(z + 1u) * x_size * y_size + (y + 1u) * x_size + (x + 1u)]),
        f32(grid.voxels[(z + 1u) * x_size * y_size + (y + 1u) * x_size + x]),
    );

    var cubeIndex = 0u;
    if (v[0] < iso_level) { cubeIndex = cubeIndex | 1u; }
    if (v[1] < iso_level) { cubeIndex = cubeIndex | 2u; }
    if (v[2] < iso_level) { cubeIndex = cubeIndex | 4u; }
    if (v[3] < iso_level) { cubeIndex = cubeIndex | 8u; }
    if (v[4] < iso_level) { cubeIndex = cubeIndex | 16u; }
    if (v[5] < iso_level) { cubeIndex = cubeIndex | 32u; }
    if (v[6] < iso_level) { cubeIndex = cubeIndex | 64u; }
    if (v[7] < iso_level) { cubeIndex = cubeIndex | 128u; }

    if (edgeTable[cubeIndex] == 0u) {
        return;
    }

    var vertList: array<vec3<f32>, 12>;
    if ((edgeTable[cubeIndex] & 1u) > 0u) { vertList[0] = interpolateVertex(iso_level, p[0], p[1], v[0], v[1]); }
    if ((edgeTable[cubeIndex] & 2u) > 0u) { vertList[1] = interpolateVertex(iso_level, p[1], p[2], v[1], v[2]); }
    if ((edgeTable[cubeIndex] & 4u) > 0u) { vertList[2] = interpolateVertex(iso_level, p[2], p[3], v[2], v[3]); }
    if ((edgeTable[cubeIndex] & 8u) > 0u) { vertList[3] = interpolateVertex(iso_level, p[3], p[0], v[3], v[0]); }
    if ((edgeTable[cubeIndex] & 16u) > 0u) { vertList[4] = interpolateVertex(iso_level, p[4], p[5], v[4], v[5]); }
    if ((edgeTable[cubeIndex] & 32u) > 0u) { vertList[5] = interpolateVertex(iso_level, p[5], p[6], v[5], v[6]); }
    if ((edgeTable[cubeIndex] & 64u) > 0u) { vertList[6] = interpolateVertex(iso_level, p[6], p[7], v[6], v[7]); }
    if ((edgeTable[cubeIndex] & 128u) > 0u) { vertList[7] = interpolateVertex(iso_level, p[7], p[4], v[7], v[4]); }
    if ((edgeTable[cubeIndex] & 256u) > 0u) { vertList[8] = interpolateVertex(iso_level, p[0], p[4], v[0], v[4]); }
    if ((edgeTable[cubeIndex] & 512u) > 0u) { vertList[9] = interpolateVertex(iso_level, p[1], p[5], v[1], v[5]); }
    if ((edgeTable[cubeIndex] & 1024u) > 0u) { vertList[10] = interpolateVertex(iso_level, p[2], p[6], v[2], v[6]); }
    if ((edgeTable[cubeIndex] & 2048u) > 0u) { vertList[11] = interpolateVertex(iso_level, p[3], p[7], v[3], v[7]); }

    for (var i = 0u; i < 16u; i=i+3) {
        if (triTable[cubeIndex * 16u + i] == -1) { break; }
        
        let v1 = vertList[u32(triTable[cubeIndex * 16u + i])];
        let v2 = vertList[u32(triTable[cubeIndex * 16u + i + 1])];
        let v3 = vertList[u32(triTable[cubeIndex * 16u + i + 2])];
        
        let v_idx = atomicAdd(&vertex_counter, 3u);
        vertices_out.vertices[v_idx * 3u + 0u] = v1.x;
        vertices_out.vertices[v_idx * 3u + 1u] = v1.y;
        vertices_out.vertices[v_idx * 3u + 2u] = v1.z;
        
        vertices_out.vertices[v_idx * 3u + 3u] = v2.x;
        vertices_out.vertices[v_idx * 3u + 4u] = v2.y;
        vertices_out.vertices[v_idx * 3u + 5u] = v2.z;

        vertices_out.vertices[v_idx * 3u + 6u] = v3.x;
        vertices_out.vertices[v_idx * 3u + 7u] = v3.y;
        vertices_out.vertices[v_idx * 3u + 8u] = v3.z;
        
        let i_idx = atomicAdd(&index_counter, 3u);
        indices_out.indices[i_idx + 0u] = v_idx;
        indices_out.indices[i_idx + 1u] = v_idx + 1u;
        indices_out.indices[i_idx + 2u] = v_idx + 2u;
    }
}